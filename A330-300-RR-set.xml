<PropertyList include="A330-main.xml">

	<sim>
	
		<sound>
			<path>Aircraft/IDG-A33X/Sounds/rrtrent-sound.xml</path>
		</sound>
	
		<description>Airbus A330-343 (RR Trent 772B-60)</description>
		<flight-model>jsb</flight-model>
		<aero>A330-300-RR</aero>
		<model>
			<path>Aircraft/IDG-A33X/Models/Article-6.xml</path>
			<livery>
				<file>White</file>
			</livery>
		</model>
		
		<chase-distance-m type="double" archive="y">-130.0</chase-distance-m>
		<view>
			<name>Pilot View</name>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.42</x-offset-m>
				<y-offset-m archive="y">1.43</y-offset-m>
				<z-offset-m archive="y">-22.6993</z-offset-m>
				<pitch-offset-deg archive="y">-12.0</pitch-offset-deg>
			</config>
		</view>
		<view n="100">
			<name>Copilot View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.42</x-offset-m>
				<y-offset-m archive="y">1.43</y-offset-m>
				<z-offset-m archive="y">-22.6993</z-offset-m>
				<pitch-offset-deg archive="y">-12.0</pitch-offset-deg>
			</config>
		</view>
		<view n="101">
			<name>Overhead Panel View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">1.47</y-offset-m>
				<z-offset-m archive="y">-22.702</z-offset-m>
				<pitch-offset-deg archive="y">38</pitch-offset-deg>
				<default-field-of-view-deg type="double">90</default-field-of-view-deg>
			</config>
		</view>
		<view n="102">
			<name>Left Front View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-3.68</x-offset-m>
				<y-offset-m archive="y">1.47</y-offset-m>
				<z-offset-m archive="y">-6.49</z-offset-m>
				<heading-offset-deg>129</heading-offset-deg>
				<default-field-of-view-deg type="double">85</default-field-of-view-deg>
			</config>
		</view>
		<view n="103">
			<name>Right Mid View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">3.68</x-offset-m>
				<y-offset-m archive="y">1.47</y-offset-m>
				<z-offset-m archive="y">4.52</z-offset-m>
				<heading-offset-deg>257</heading-offset-deg>
				<default-field-of-view-deg type="double">85</default-field-of-view-deg>
			</config>
		</view>
		<view n="104">
			<name>Left Aft View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-3.68</x-offset-m>
				<y-offset-m archive="y">1.47</y-offset-m>
				<z-offset-m archive="y">11.6</z-offset-m>
				<heading-offset-deg>75</heading-offset-deg>
				<default-field-of-view-deg type="double">85</default-field-of-view-deg>
			</config>
		</view>
		
		<systems n="0">
            <autopilot n="0">
                <path>Aircraft/IDG-A33X/Systems/fadec-rr.xml</path>
            </autopilot>
		</systems>
		
	</sim>
	
	<options n="0">
		<!--EIS2 type="bool">0</EIS2-->
		<eng>RR</eng>
		<maxblock>215.0</maxblock>
	</options>
	
	<MCDUC n="0">
		<type>A330-343</type>
		<eng>RR772B-60</eng>
	</MCDUC>
	
	<nasal>
		<specific>
			<file>Aircraft/IDG-A33X/Nasal/a333livery-rr.nas</file>
		</specific>
		<fadec>
			<file>Aircraft/IDG-A33X/Nasal/fadec-pwrr.nas</file>
		</fadec>
	</nasal>

</PropertyList>
