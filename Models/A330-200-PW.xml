<PropertyList>
	<name>A330-223</name>
	<path>empty.ac</path>

	<model>
		<name>A330-223 Main</name>
		<path>Aircraft/IDG-A33X/Models/A330-223-main.xml</path>
	</model>

<!-- LIVERY SELECT -->
	<!-- Animation -->
	<animation>
		<type>material</type>
		<!-- Fuselage -->
		<object-name>fuselage</object-name>
		<object-name>nosecone</object-name>
		<object-name>vstab</object-name>
		<object-name>rudder</object-name>
		<object-name>bellyfairing</object-name>
		<!-- Wings -->
		<object-name>winglet0</object-name>
		<object-name>winglet1</object-name>
		<!-- Engines -->
		<object-name>pw.nacelle0</object-name>
		<object-name>pw.reverser0</object-name>
		<object-name>pw.nacelle1</object-name>
		<object-name>pw.reverser1</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture</texture-prop>
	</animation>

	<!-- Nasal code -->
	<nasal>
		<load>
		 var livery_update = aircraft.livery_update.new("Aircraft/IDG-A33X/Models/Liveries/A332PW/");
		</load>
		<unload>
		 livery_update.stop();
		</unload>
	</nasal>

</PropertyList>
