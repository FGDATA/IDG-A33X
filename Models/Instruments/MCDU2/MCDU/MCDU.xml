	<!-- Colors -->
	<!-- White -->
    <animation>
        <type>material</type>
		<object-name>page</object-name>
		<object-name>aidstext</object-name>
		<object-name>fmgctext</object-name>
		<object-name>acarstext</object-name>
		<object-name>cfdstext</object-name>
		<object-name>return</object-name>
        <diffuse>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </diffuse>
        <ambient>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </ambient>
        <specular>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </specular>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
	
